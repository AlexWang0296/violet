title = {{An electron microscope study of mechanical twinning and fracture in TiAl alloys}},
url = {http://www.tandfonline.com/doi/abs/10.1080/14786430412331315662},
volume = {85},
year = {2005}
}
@article{Zhang2017c,
abstract = {Background: In Switzerland approximately 30{\%} of hepatitis C virus (HCV) infected individuals have been diagnosed. To enhance this rate, new detection strategies are needed. The aim of this project was to analyze the distribution of HCV cases in Switzerland to develop better detection strategies. Methods: A previously described HCV disease burden model was populated with Swiss assumptions regarding HCV prevalence, viremic rate and age distribution. The viremic population was aged from 1998 to 2013 accounting for mortality and cure rates, and was stratified according to birth cohort. The number of screenings required to identify one viremic case was calculated as 1/(viremic HCV prevalence). Costs in Swiss Francs (CHF) associated with diagnosing and genotyping one viremic case were estimated as follows - [CHF 25*({\#} anti-HCV tests administered)] + [CHF 180*({\#} HCV-RNA tests administered for anti-HCV positive cases)] + [CHF 180*({\#} genotype tests administered for HCV-RNA positive cases)]. Results: More than 50{\%} of the viremic HCV infected population was born during 1954-1973, with 73{\%} born during 1949-1978. Random screening would require {\textgreater}100 anti-HCV tests (in addition to further confirmatory tests) to find one current HCV case, while screening in the 1949-1978 cohort (36-65 years of age) could find one case per 59 persons screened. Screening in the 1949-1978 cohort would cost CHF 1,890 (USD {\$}2,110) per positive diagnosis, as compared with a CHF 2,790 ({\$}3,115) cost for random screening. Screening by 5-year cohort could find anywhere from one case per 56 screened (1974-1978 cohort, 12{\%} of the viremic population) to one case per 5,855 screened (2009-2013 cohort, {\textless}1{\%} of the viremic population) Conclusion: The most efficient birth-year screening strategy would target persons born between 1949 and 1978 (35 - 64 years of age), as this cohort accounts for over 70{\%} of viremic cases and requires screening of only 59 individuals to identify one viremic HCV infected case. Targeted birth cohort screening is more efficient and cost effective than random screening. (Table Presented).},
