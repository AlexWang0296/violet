doi = {10.3390/met7100432},
file = {:home/alex/Documents/Mendeley Desktop/Metals/2017{\_}Molecular Dynamics Simulation of Crack Propagation in Nanoscale Polycrystal Nickel Based on Different Strain Rates.pdf:pdf},
issn = {2075-4701},
journal = {Metals},
keywords = {crack growth,deformation,metals,molecular dynamics,polycrystal},
number = {10},
pages = {432},
title = {{Molecular Dynamics Simulation of Crack Propagation in Nanoscale Polycrystal Nickel Based on Different Strain Rates}},
url = {http://www.mdpi.com/2075-4701/7/10/432},
volume = {7},
year = {2017}
}
