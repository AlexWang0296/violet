@article{Edwards2017,
abstract = {In this paper the occurrence of twinning parallel to the lamellae during compression at 700 °C of a polycrystalline nearly lamellar commercial $\gamma$-TiAl alloy, Ti-45Al-2Nb-2Mn(at{\%})-0.8 vol{\%} TiB2, has been studied and shown to lead to the formation of cracks at colony boundaries. However, the occurrence of this longitudinal twinning mode was less common by at least a factor of ten in tests at room temperature. Furthermore, the debonding of colony boundaries caused by the shear strain of longitudinal twinning is exacerbated when the same $\gamma$-TiAl variant favourably oriented for twinning occurs repeatedly in the lamellar structure. This effect was caused by the preferential nucleation of certain $\gamma$-TiAl variants in the presence of TiB2boride reinforcement. It is shown that the boride additions increase the probability of double, triple or even higher order multiply stacked $\gamma$-variants. This increases the resulting shear strain that must be accommodated and hence the probability of crack nucleation.},
author = {Edwards, Thomas Edward James and {Di Gioacchino}, Fabio and Mu{\~{n}}oz-Moreno, Roc{\'{i}}o and Clegg, William John},
doi = {10.1016/j.actamat.2017.08.055},
file = {:home/alex/Documents/Mendeley Desktop/Acta Materialia/2017{\_}The interaction of borides and longitudinal twinning in polycrystalline TiAl alloys.pdf:pdf},
issn = {13596454},
journal = {Acta Materialia},
keywords = {Deformation twinning,Digital image correlation,Electron backscattering diffraction (EBSD),Titanium aluminides},
month = {nov},
pages = {305--316},
publisher = {Pergamon},
title = {{The interaction of borides and longitudinal twinning in polycrystalline TiAl alloys}},
url = {https://www.sciencedirect.com/science/article/pii/S135964541730719X},
volume = {140},
year = {2017}
}
@article{void.creep,
abstract = {An investigation on the effect of aging on the tensile properties and creep behavior of a fully lamellar (FL) Ti-47Al-2Nb-1Mn-0.5W-0.5Mo-0.2Si (at.{\%}) alloy is presented. Aging at intermediate temperatures produces precipitate particles predominantly along $\alpha$2/$\gamma$ lamellar interfaces or within $\alpha$2 lamellae. Most of the precipitates are $\beta$-particles with fewer silicides. Aging at intermediate temperatures leads to increased tensile strength as a result of precipitation of $\beta$ and silicide particles. In addition, aging yields an increased primary creep resistance but a shortened creep life for the FL structure. The shortened creep life of the aged FL structures could be a result of early decomposition of $\alpha$2 lamellae during creep. {\textcopyright} 2002 Elsevier Science B.V. All rights reserved.},
