doi = {10.1080/014186198253778},
file = {:home/alex/Documents/Mendeley Desktop/Philosophical Magazine A Physics of Condensed Matter, Structure, Defects and Mechanical Properties/1998{\_}Activation of slip in lamellae of $\alpha$2-Ti3Al in TiAl alloys.pdf:pdf},
