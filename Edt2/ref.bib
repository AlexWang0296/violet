@article{Zhao1991,
abstract = {Ti3Al/TiAl interfaces and TiAl/TiAlT thermal twin boundaries were investigated by transmission electron microscopy in a Ti-42 at.{\%}Al as-cast and homogenized alloy. Intrinsic and extrinsic interfacial dislocations (IIDs and EIDs respectively) have been found on the Ti3Al/TiAl interfaces. The IIDs consist of two types of arrays, one of which predominantly aligns along [112] of TiAl, undergoing the dissociation: 1 2[110] → 1 6[211] + (ABP′ + SISF′) + 1 6[121]. Another type aligns close to [110] of TiAl with Burgers vector 1 6[112]. The characteristics of the IIDs are associated with lattice misfits on the interfaces. The EIDs appear with arbitrary alignment and undergo various dissociations, which have no correlation with the interfacial misfits. It is suggested that the EIDs are generated both on the interfaces and also from the interior of Ti3Al or TiAl lamellae. The interaction between IIDs and EIDs results in a distorted IID configuration that is subject to the variation of the planar faults bounded by the dissociated IID and EID partials. Thermal twin plates in the lamellar structure are always present neighbouring Ti3Al lamellae. Split triple-partials of 1 6{\textless}112] were resolved on the TiAl/TiAlT thermal twin boundaries as opposed to the singly present partials on deformation twin boundaries in TiAl. It is suggested that the thermal twinning is related to the triplets structure which is probably generated by the dissociation of 1 2{\textless}112] superdislocations. The potential influence of the interfaces on the mechanical properties of such two-phase alloys has been discussed. {\textcopyright} 1991.},
author = {Zhao, L. and Tangri, K.},
doi = {10.1016/0956-7151(91)90003-J},
file = {:home/alex/Documents/Mendeley Desktop/Acta Metallurgica Et Materialia/1991{\_}TEM investigation on the interfacial boundaries in as-cast Ti3AlTiAl alloy.pdf:pdf},
issn = {09567151},
journal = {Acta Metallurgica Et Materialia},
number = {10},
pages = {2209--2224},
title = {{TEM investigation on the interfacial boundaries in as-cast Ti3Al+TiAl alloy}},
volume = {39},
year = {1991}
}
