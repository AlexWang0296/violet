issn = {01418610},
journal = {Philosophical Magazine A: Physics of Condensed Matter, Structure, Defects and Mechanical Properties},
number = {1},
pages = {217--238},
title = {{Activation of slip in lamellae of $\alpha$2-Ti3Al in TiAl alloys}},
