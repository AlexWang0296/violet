@article{Botten2001,
abstract = {Samples of fully lamellar Ti44Al8Nb1B and Ti44Al4Nb4Hf0.2Si have been tested in tension at room temperature while they were interfaced to an acoustic emission system. The generation of acoustic signals, at stresses well below the 0.2{\%} yield stress and at higher stresses, has been shown to be due to the formation of cracks within individual lamellar colonies. Data obtained in the micro-strain region in loading-unloading measurements confirmed that there is significant plastic flow at stresses 200 MPa below the 0.2{\%} proof stress of 625 MPa, and it is inferred that this flow gives rise to the cracking. The mechanism whereby such cracking occurs and the possible influence of these cracks on subsequent fatigue behaviour are discussed. {\textcopyright} 2001 Acta Materialia Inc. Published by Elsevier Science Ltd.},
author = {Botten, R. and Wu, X. and Hu, D. and Loretto, M. H.},
doi = {10.1016/S1359-6454(01)00091-X},
file = {:home/alex/Documents/Mendeley Desktop/Acta Materialia/2001/2001{\_}Acta Materialia{\_}The significance of acoustic emission during stressing of TiAl-based alloys. Part I Detection of cracking during lo.pdf:pdf},
issn = {13596454},
journal = {Acta Materialia},
keywords = {Acoustic,Fatigue,Titanium alloys,Yield},
number = {10},
pages = {1687--1691},
title = {{The significance of acoustic emission during stressing of TiAl-based alloys. Part I: Detection of cracking during loading up in tension}},
volume = {49},
year = {2001}
