@article{Meriaux2010,
abstract = {In this study, a fretting fatigue test has been equipped with an acoustic emission (AE) device in order to identify the successive crack propagation mechanisms. The fretting fatigue crack nucleation and propagation is a complicated process. Cracks initiate and propagate firstly due to shearing (mode II) and then by tension (mode I). The crack propagation generates mechanical energy emission. Elastic waves appear and can be detected through AE. A complete analysis of the AE signals (multi-parameter analysis, location of the AE in the loading cycle and a statistical analysis) led to an identification of three different steps in the crack propagation process. The evolution of the shearing and the tension influences in the crack propagation process is recognizable separately. Therefore, the three crack propagation steps have been identified as (a) crack propagation in mode II, (b) mixed mode crack propagation and (c) pure mode I crack propagation. {\textcopyright} 2010 Published by Elsevier Ltd.},
author = {Meriaux, J. and Boinet, M. and Fouvry, S. and Lenain, J. C.},
doi = {10.1016/j.triboint.2010.06.009},
file = {:home/alex/Documents/Mendeley Desktop/Tribology International/2010/2010{\_}Tribology International{\_}Identification of fretting fatigue crack propagation mechanisms using acoustic emission.pdf:pdf},
isbn = {0301-679X},
issn = {0301679X},
journal = {Tribology International},
keywords = {Acoustic emission,Crack propagation,Fretting fatigue,Ti6Al4V},
number = {11},
pages = {2166--2174},
publisher = {Elsevier},
title = {{Identification of fretting fatigue crack propagation mechanisms using acoustic emission}},
url = {http://dx.doi.org/10.1016/j.triboint.2010.06.009},
volume = {43},
year = {2010}
}
