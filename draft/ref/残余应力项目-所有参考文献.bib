number = {11},
pages = {1737--1743},
publisher = {Springer},
title = {{Observations on microstructurally sensitive fatigue crack growth in a widmanst{\"{a}}tten Ti-6Al-4V alloy}},
url = {http://link.springer.com/10.1007/BF02646877},
volume = {8},
year = {1977}
}
