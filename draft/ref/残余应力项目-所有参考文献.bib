@article{Elbestawi1996,
abstract = {Saw-toothed chips are formed during machining of hardened steel (HRC∞ 60-63). This paper presents a new analytical approach for modelling the chip formation mechanism in hard-turning. It has been observed that the chip formation starts with initiation of a crack at the free surface of the workpiece which further propagates towards the cutting edge of the tool. The crack soon ceases to grow at a point where severe plastic deformation of the material exists under higher level of compressive stresses. The chip segment caught up between the tool rake face and the crack is pushed out while the material in the plastic region just below the base of the crack is displaced along the tool rake face thus forming saw-toothed chips. The direction of crack initiation and propagation are predicted using the surface layer energy/strain energy density criterion. The maximum value of surface layer energy, Yemaxcan be used to evaluate the angle of crack initiation while the strain energy density criterion predicts the corresponding crack propagation angle. Here, the process of chip formation is considered to be a mixed mode crack problem of Mode I and Mode II. The theoretical predictions are verified by the resultant chip contours obtained experimentally. The predictions made are shown to be in good agreement with those measured experimentally. {\textcopyright} 1996 CIRP.},
author = {Elbestawi, M. A. and Srivastava, A. K. and El-Wardany, T. I.},
doi = {10.1016/S0007-8506(07)63019-4},
isbn = {0007-8506},
issn = {17260604},
journal = {CIRP Annals - Manufacturing Technology},
keywords = {Chip formation,Crack,Hard turning,Strain energy density},
number = {1},
pages = {71--76},
publisher = {Elsevier},
title = {{A Model for Chip Formation During Machining of Hardened Steel}},
volume = {45},
year = {1996}
}
@article{VonFieandt2018,
abstract = {Two Ti(C,N) coatings were tested by means of micro abrasion and scratch testing. The coatings differed in grain size, orientation ({\textless}111{\textgreater} and {\textless}111{\textgreater}, {\textless}311{\textgreater} and {\textless}211{\textgreater} respectively) and hardness (36 GPa and 23 GPa respectively). The {\textless}111{\textgreater} oriented coating had a 20{\%} higher wear resistance compared to the reference coating when abraded with 1 $\mu$m diamonds. When abraded with 6 $\mu$m diamonds the abrasion resistance of the reference coating was superior compared to the {\textless}111{\textgreater} oriented coating by 36{\%}. Furthermore, it was found that the {\textless}111{\textgreater} oriented coating had 35{\%} better adhesion compared to the reference. The improved mechanical properties of the {\textless}111{\textgreater} oriented coating was attributed to a high degree of orientation and the higher hardness.},
author = {von Fieandt, L. and Fallqvist, M. and Larsson, T. and Lindahl, E. and Boman, M.},
doi = {10.1016/j.triboint.2017.11.040},
issn = {0301679X},
