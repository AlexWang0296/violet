pages = {374--386},
publisher = {Wiley Online Library},
title = {{Nonlinear numerical study of crack initiation and propagation in a reactor pressure vessel under pressurized thermal shock using XFEM}},
url = {http://doi.wiley.com/10.1111/ffe.12689},
volume = {41},
year = {2018}
}
