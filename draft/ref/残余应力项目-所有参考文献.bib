journal = {Acta Materialia},
keywords = {Crack propagation,Fatigue,Fracture,Metallic glasses,Zirconium alloys},
pages = {165--175},
publisher = {Elsevier},
title = {{Fatigue endurance limit and crack growth behavior of a high-toughness Zr{\textless}inf{\textgreater}61{\textless}/inf{\textgreater}Ti{\textless}inf{\textgreater}2{\textless}/inf{\textgreater}Cu{\textless}inf{\textgreater}25{\textless}/inf{\textgreater}Al{\textless}inf{\textgreater}12{\textless}/inf{\textgreater} bulk metallic glass}},
