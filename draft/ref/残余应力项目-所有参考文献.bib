issn = {1364503X},
journal = {Philosophical Transactions of the Royal Society A: Mathematical, Physical and Engineering Sciences},
keywords = {Cyclic slip irreversibilities,Fatigue crack growth,Fatigue crack initiation,Fatigue damage,Fatigue life,Metal fatigue},
number = {2038},
pages = {20140132--20140132},
pmid = {25713457},
publisher = {The Royal Society},
title = {{Microstructural mechanisms of cyclic deformation, fatigue crack initiation and early crack growth}},
url = {http://rsta.royalsocietypublishing.org/cgi/doi/10.1098/rsta.2014.0132},
volume = {373},
year = {2015}
}
