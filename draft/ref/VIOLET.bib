issn = {09270256},
journal = {Comput. Mater. Sci.},
keywords = {Molecular dynamics simulation,Shape memory alloy,Shape memory effect,Superelasticity},
pages = {61--69},
publisher = {Elsevier B.V.},
title = {{Molecular dynamics simulation on the shape memory effect and superelasticity in NiTi shape memory alloy}},
url = {https://doi.org/10.1016/j.commatsci.2018.01.026},
volume = {146},
year = {2018}
