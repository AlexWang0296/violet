year = {2015}
}
@article{Escaig1968,
abstract = {Duesbery et Hirsch ont sugg{\'{e}}r{\'{e}} r{\'{e}}cemment que dans les structures c.c. et h.c., la d{\'{e}}viation d'une dislocation hors de son plan de dissociation, doit se produire {\`{a}} faible contrainte par un m{\'{e}}canisme de double d{\'{e}}crochement. On montre ici que ce m{\'{e}}canisme doit se produire en dessous d'une contrainte {\ss}c = R/b2, o{\`{u}} R est l'{\'{e}}nergie de recombinaison et b le vecteur de Burgers de la dislocation; de plus, il n'introduit aucune discontinuit{\'{e}} de contrainte, d'{\'{e}}nergie, ou de volume d'activation. On calcule l'{\'{e}}nergie d'activation maximum de la d{\'{e}}viation correspondant{\`{a}} une composante thermique de la contrainte nulle, pour laquelle on retrouve l'expression donn{\'{e}}e par Duesbery et Hirsch. En tenant compte de l'interaction {\'{e}}lastique des d{\'{e}}crochements au moyen d'une tension de ligne qui varie logarithmiquement aves leur distance de s{\'{e}}paration, on obtient un volume d'activation qui varie en $\sigma$−1 {\`{a}} faible contrainte $\sigma$.},
