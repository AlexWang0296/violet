doi = {10.1103/PhysRevB.92.134107},
file = {:home/alex/Documents/Mendeley Desktop/Ko, Grabowski, Neugebauer/Physical Review B - Condensed Matter and Materials Physics/2015{\_}Development and application of a Ni-Ti interatomic potential with high predictive accuracy of the martensitic phase transition.pdf:pdf},
isbn = {1098-0121 1550-235X},
