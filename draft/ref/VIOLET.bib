file = {:C$\backslash$:/DOC/Mendeley Desktop/Computational Materials Science/2011/2011{\_}An analysis on nanovoid growth in body-centered cubic single crystalline vanadium.pdf:pdf},
issn = {09270256},
journal = {Comput. Mater. Sci.},
number = {8},
pages = {2411--2421},
title = {{An analysis on nanovoid growth in body-centered cubic single crystalline vanadium}},
volume = {50},
year = {2011}
}
@article{Xiong2015,
abstract = {Dislocation interactions with distributed condensed vacancy clusters in fcc metals were simulated via a concurrent atomistic-continuum method. Due to void strengthening, the dislocation lines are found to bow as a result of pinning on the original glide plane and undergo depinning through drawing out screw dipoles and forming prismatic loops on the secondary slip plane. We discovered an inertia-induced transition between Hirsch looping and void shearing mechanisms as the void spacing ranges from the scale of nm to hundreds of nm. Contrary to prior understanding, simulations suggest that large voids (∼5 nm in diameter) can behave as weak barriers to dislocation motions under high strain-rate dynamic conditions.},
