journal = {Intermetallics},
keywords = {B. phase diagram,Oxidation},
month = {aug},
number = {8},
pages = {841--848},
publisher = {Elsevier},
title = {{Formation of nickel aluminide coating on $\gamma$-TiAl alloy}},
url = {https://www.sciencedirect.com/science/article/pii/S0966979503000839},
volume = {11},
year = {2003}
}
@article{Es-souni1995,
abstract = {The creep deformation behaviour and creep microstructures of a dual phase TiAl/Ti3Al alloy Ti48Al2Cr (at.{\%}) with a fully transformed lamellar structure have been investigated at 800°C in tension creep. The creep curves show no steady state creep rate, rather a minimum creep rate was obtained followed by a steep increase in the creep rate. The application of a power-law creep equation of the type dot $\epsilon${\{}lunate{\}} = A$\sigma$nleads to a stress exponent n = 7.6. The deformation microstructures show no deformation twins and essentially consist of 1 2{\textless}110] dislocations; recovered structures were observed in the fine $\gamma$-grains at colony boundaries whereas long and curved 1 2{\textless}110] segments were found in the $\gamma$-lamellae. These microstructural observations suggest a dislocation controlled creep; it ensues that the stress exponent obtained using power-law creep does not reflect the operating creep mechanism. This is attributed to early nucleation and growth of voids and cavities at colony boundaries which do not allow steady-state creep to be established, an underlying condition for the applicability of the power-law equation above. The nucleation and growth of void and cavities are thought to arise as a result of stress buildup at colony boundaries which in turn results from the dependence of deformation behaviour on the orientation of the lamellar colonies with respect to the tensile axis. The loss of creep strength is found to be primarily due to growth and coalescence of these voids and cavities. {\textcopyright} 1994.},
author = {Es-souni, M. and Bartels, A. and Wagner, R.},
