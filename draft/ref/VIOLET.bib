doi = {10.1016/j.commatsci.2011.03.019},
file = {:home/alex/Documents/Mendeley Desktop/Xu et al/Computational Materials Science/2011{\_}An analysis on nanovoid growth in body-centered cubic single crystalline vanadium.pdf:pdf},
issn = {09270256},
journal = {Computational Materials Science},
