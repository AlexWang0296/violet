file = {:C$\backslash$:/DOC/Mendeley Desktop/Metallography, Microstructure, and Analysis/2012/2012{\_}Microstructures for Two-Phase Gamma Titanium Aluminide Fabricated by Electron Beam Melting.pdf:pdf},
isbn = {2192-9262},
issn = {21929262},
journal = {Metallogr. Microstruct. Anal.},
keywords = {Electron beam melting,Hardness measurements,Metallography and microstructure analysis,Two-phase TiAl},
number = {1},
pages = {14--27},
title = {{Microstructures for Two-Phase Gamma Titanium Aluminide Fabricated by Electron Beam Melting}},
volume = {1},
year = {2012}
