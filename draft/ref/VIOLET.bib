journal = {Metallography, Microstructure, and Analysis},
keywords = {Electron beam melting,Hardness measurements,Metallography and microstructure analysis,Two-phase TiAl},
number = {1},
pages = {14--27},
title = {{Microstructures for Two-Phase Gamma Titanium Aluminide Fabricated by Electron Beam Melting}},
