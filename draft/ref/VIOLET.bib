doi = {10.1016/S1359-6454(00)00312-8},
file = {:C$\backslash$:/DOC/Mendeley Desktop/Acta Materialia/2001/2001{\_}Room temperature mechanical behavior of silicon-doped TiAl alloys with grain sizes in the nano- and submicron-range.pdf:pdf},
