pages = {304--313},
publisher = {Elsevier Ltd},
title = {{Morphology and stability of orthorhombic and hexagonal phases in a lamellar $\gamma$-Ti-42Al-8.5Nb alloy-A transmission electron microscopy study}},
volume = {135},
