keywords = {fracture,gamma TiAl alloy,molecular dynamics,tensile deformation,tilt grain boundary},
number = {11},
pages = {3645--3651},
title = {{Molecular dynamics simulation of fracture behaviors of {\textless}110{\textgreater} tilt grain boundaries in $\gamma$-TiAl}},
volume = {24},
year = {2014}
}
@article{LIU2017603,
abstract = {BiVO4/WO3heterojunction film is considered as a very promising candidate for photoelectrochemical water splitting. However, the BiVO4/WO3films prepared by a conventional method exhibit numerous grain boundaries in the BiVO4layer and a weak contact at the heterojunction interface. In this paper, we found that BiVO4could be formed from hydrothermal Bi2WO6in the weak acid solution dissolving V2O5and successfully developed an in situ transformation method to prepare BiVO4/WO3heterojunction films. The prepared BiVO4/WO3heterojunction films were characterized in detail and showed a high quality core/shell WO3/BiVO4structure. The formation mechanism of the BiVO4layer on the surface of WO3film has been put forward. In particular, the prepared BiVO4/WO3films were used as photoanodes and showed an excellent visible light response, a good photostability and high photoelectrochemical activity. The photocurrent density for BiVO4/WO3photoanode was up to 2.92 mA cm−2at 1.81 VRHEwithout additional electrocatalyst or doping under visible light illumination.},
