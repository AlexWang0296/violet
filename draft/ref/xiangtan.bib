pages = {19--25},
title = {{Non-equilibrium microstructure of Li1.4Al0.4Ti1.6(PO4)3superionic conductor by spark plasma sintering for enhanced ionic conductivity}},
url = {http://www.sciencedirect.com/science/article/pii/S2211285518304439},
volume = {51},
