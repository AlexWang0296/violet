journal = {Surface and Coatings Technology},
keywords = {Failure,Interfacial morphology,Internal oxidation,Residual stress,Thermal barrier coatings},
pages = {215--226},
publisher = {Elsevier},
title = {{Effect of internal oxidation on the interfacial morphology and residual stress in air plasma sprayed thermal barrier coatings}},
volume = {334},
