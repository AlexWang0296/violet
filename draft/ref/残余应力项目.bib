journal = {International Journal of Fatigue},
keywords = {Environment,Fatigue crack growth,Titanium,Ultrasonic fatigue},
pages = {154--162},
publisher = {Elsevier},
title = {{Investigating environmental effects on small fatigue crack growth in Ti-6242S using combined ultrasonic fatigue and scanning electron microscopy}},
volume = {70},
year = {2015}
}
