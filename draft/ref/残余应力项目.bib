@article{Gilbert1998,
abstract = {An experimental study is made of the cyclic fatigue behavior of a bulk metallic glass alloy, Zr/sub 41.2/Ti/sub 13.8/Cu/sub 12.5/Ni/sub 10/Be/sub 22.5/ (at{\%}), involving both crack propagation (using precracked C(T) specimens) and stress-life (S/N) (using nominally crack-free, unnotched beams). Crack-propagation behavior in the metallic glass was found to be similar to that observed in traditional ductile crystalline alloys (e.g., high-strength steels and aluminum alloys), both in terms the dependence of growth rates on the applied stress-intensity range and in the presence of ductile striations on fatigue fracture surfaces. Conversely, the stress-life behavior of the metallic glass (at R°0) was very different from that observed in the crystalline alloys. Fatigue lifetimes were much shorter in the amorphous alloy and exhibited a far lower dependence on the applied stress range; moreover, no evidence of a fatigue limit could be detected until stress amplitudes dropped below °1/25 of the tensile strength. Given the similarity in crack-growth behavior but the very different stress/life properties, the prime distinction in the cyclic fatigue behavior of ductile crystalline and bulk amorphous metals appears to be that the ease of crack initiation in the metallic glass is far less sensitive to the applied stress},
author = {Gilbert, C. J. and Lippmann, J. M. and Ritchie, R. O.},
doi = {10.1016/S1359-6462(97)00522-8},
isbn = {1359-6462},
issn = {13596462},
journal = {Scripta Materialia},
number = {4},
pages = {537--542},
publisher = {Elsevier},
title = {{Fatigue of a Zr-Ti-Cu-Ni-Be bulk amorphous metal: Stress/life and crack-growth behavior}},
volume = {38},
year = {1998}
}
@article{Cano2018,
abstract = {To predict the fatigue crack growth in full-scale components made of polymeric materials, a methodology to transfer laboratory crack-growth data to structures is needed. This requires the similitude hypothesis to be applicable using a valid crack driving force, which allows the description of the R-ratio effect on the crack growth behavior. Whereas in metals it is common to use the range in the applied stress intensity factor, $\Delta$K, as the crack driving force, in polymeric materials and composites the use of the energy release rate G has been considered and $\Delta$G has been proposed as the crack driving force to ensure the similitude principle. Therefore, the aim of this paper is to assess the validity of $\Delta$G as a proper crack driving force and to explore the way of practical application in an amorphous thermoplastic copolyester Polyethylene terephthalate glycol (PET-G).},
author = {Cano, A. J. and Salazar, A. and Rodr{\'{i}}guez, J.},
doi = {10.1016/j.ijfatigue.2017.10.013},
issn = {01421123},
journal = {International Journal of Fatigue},
keywords = {Copolyester Polyethylene terephthalate glycol (PET,Crack driving force,Fatigue crack growth,Hartman-Schijve,R-ratio},
pages = {27--32},
publisher = {Elsevier},
title = {{Evaluation of different crack driving forces for describing the fatigue crack growth behaviour of PET-G}},
volume = {107},
year = {2018}
}
@article{Xie2010,
abstract = {The 2D model of edge dislocations generation from blunt crack tip in viscoelastic material under residual stress has been proposed, the solution of stress field and displacement field are solved by using complex potential method, conformal mapping and Laplace inverse transformation. The explicit expressions of stress intensity factor, strain energy density and crack tip slide displacement are obtained in closed form. The principle of compatibility of blunt crack to edge dislocations has been used to evaluate the dislocations number and dimensionless ratio $\alpha$. Numerical results present that the number of edge dislocations first increases and then decreases with increase of zone size ratio of the dislocations zone and none-dislocations zone, but it can be reduced by higher configurations ratio of semi-minor axis and semi-major axis. In addition, it increases with time and tends to be a constant quickly. The normalized multiplier $\alpha$ first increases and then decreases with increase of zone size ratio. In addition, it decreases with time and the increase of crack configurations ratio. Both normalized micro-volume SED and normalized dislocation-volume SED decrease with increase of distance from crack tip and tend to vanish. But the dislocation-volume SED decreases more quickly than micro-volume SED does, because of its stronger singularity. Moreover, they increase with time and decrease of configurations ratio. {\textcopyright} 2010.},
author = {Xie, C. and Fang, Q. H. and Jiang, C. Z. and Liu, Y. W.},
doi = {10.1016/j.tafmec.2010.10.003},
