journal = {Acta Materialia},
keywords = {Crack-dislocation interaction,Dislocation mobility,Fracture,Semiconductor,Synchrotron radiation},
month = {jun},
number = {10},
pages = {3864--3871},
title = {{Interaction of a dislocation with a crack tip: From stimulated emission to avalanche generation}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1359645411001571},
volume = {59},
year = {2011}
}
