keywords = {Fretting wear,Friction coefficient,Plasma-sprayed coating,Wear resistance},
pages = {196--207},
publisher = {Elsevier},
title = {{Fretting wear properties of plasma-sprayed Ti3SiC2coatings with oxidative crack-healing feature}},
url = {https://ac.els-cdn.com/S0301679X17304693/1-s2.0-S0301679X17304693-main.pdf?{\_}tid=347e9436-f5c1-11e7-baf9-00000aab0f26{\&}acdnat=1515559770{\_}ff9115404b46594e8b9ebbedcbd0ae14},
