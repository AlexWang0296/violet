journal = {Acta Materialia},
keywords = {Dislocation,Fracture,Mechanical properties,Modeling,Simulation},
month = {oct},
number = {18},
pages = {5933--5940},
title = {{Dislocation shielding and crack tip decohesion at the atomic scale}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1359645410004337},
volume = {58},
year = {2010}
}
