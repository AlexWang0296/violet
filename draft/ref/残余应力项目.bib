doi = {10.1016/j.ijfatigue.2016.09.015},
issn = {01421123},
journal = {International Journal of Fatigue},
keywords = {Bauschinger effect,Dislocation pile-up,Fatigue crack growth,Fatigue crack initiation,Micromechanics},
