issn = {14602695},
journal = {Fatigue and Fracture of Engineering Materials and Structures},
keywords = {fatigue crack growth,residual stress,superposition rule,weld joints},
number = {8},
pages = {1227--1239},
publisher = {Wiley Online Library},
title = {{A modified loading method for separating the effect of residual stress on fatigue crack growth rate of welded joints}},
url = {http://doi.wiley.com/10.1111/ffe.12617},
volume = {40},
year = {2017}
}
@article{Zhang2018,
abstract = {This paper investigated the fatigue life of a dissimilar welded joint between SAF2205 duplex stainless steel and 304 austenite stainless steel. A nonlinear fatigue damage model based on continuum damage mechanics (CDM) is used to estimate the fatigue life, and the effect of the residual stress is studied. The results show that large residual stresses were generated in the dissimilar welded joint and agree well with the indentation measurement results. The fatigue test specimens were cut from the welded sample, and the residual stress relaxed significantly after longitudinal cutting, especially for longitudinal residual stress. Considering the transverse residual stress, the estimated fatigue life is more consistent with the experiment. The residual stress mainly affects the mean stress, rather than the stress amplitude. As the longitudinal residual stress relaxed significantly after cutting, the effect of welding residual stress on fatigue life can be seriously overestimated. The width of fatigue test specimen should be at least 30{\%} of the as-weld sample for fatigue design in consideration of the residual stress relaxation.},
author = {Zhang, Weiya and Jiang, Wenchun and Zhao, Xu and Tu, Shan Tuang},
doi = {10.1016/j.ijfatigue.2018.01.002},
issn = {01421123},
journal = {International Journal of Fatigue},
keywords = {Continuum damage mechanics,Fatigue life,Residual stress,Welded joint},
pages = {182--190},
publisher = {Elsevier},
title = {{Fatigue life of a dissimilar welded joint considering the weld residual stress: Experimental and finite element simulation}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0142112318300021},
volume = {109},
year = {2018}
}
