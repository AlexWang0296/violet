issn = {00432288},
journal = {Welding in the World},
keywords = {Elastoplastic analysis,Finite element analysis,Multirun welding,Peening,Residual stresses},
month = {may},
number = {3},
pages = {517--527},
publisher = {Springer Berlin Heidelberg},
title = {{Numerical analysis of residual stress distribution on peening process}},
url = {http://link.springer.com/10.1007/s40194-017-0430-4},
