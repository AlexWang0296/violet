publisher = {Trans Tech Publications},
title = {{Numerical Simulation and Response Analysis of Residual Stress Induced by Micro-Scale Laser Shock Peening in TiN Film}},
url = {http://www.scientific.net/AMR.452-453.741},
volume = {452-453},
