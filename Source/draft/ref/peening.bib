issn = {09215093},
journal = {Materials Science and Engineering A},
keywords = {Fatigue life,Residual stress relaxation,Ti6Al4V titanium alloy,Warm laser peening,$\alpha$ phase},
number = {October 2016},
pages = {297--304},
publisher = {Elsevier},
title = {{Residual stress relaxation and its effects on the fatigue properties of Ti6Al4V alloy strengthened by warm laser peening}},
url = {http://dx.doi.org/10.1016/j.msea.2016.10.073},
volume = {680},
year = {2017}
}
@article{Cai2006,
abstract = {Mechanical surface treatment by shot peening followed by aging at 700 and 740 °C was performed on Inconel 718. A previously proposed XRD method (Ref 10) for the quantitative phase analysis of Inconel 718 allowed for the determination of the precipitation kinetics of the ␥؆ phase in the shot-peened layer and the matrix, respectively. The residual compressive stress field induced by shot peening and its relaxation behavior during aging were also determined. The relaxation process can be described by the Zener-Wert-Avrami function. The precipitation rate in the ␥؆ phase in the shot-peened layer is greatly accelerated, which causes differences in the ␥؆ phase amounts between the skin and the core during aging, especially during the initial stage. The high precipitation rate of the ␥؆ phase in the shot-peened layer can be interpreted by the nonequilibrium segregation of niobium.},
