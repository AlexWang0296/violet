issn = {01421123},
journal = {International Journal of Fatigue},
keywords = {Fatigue life,Non-conventional machining,Shot peening,Surface integrity,Titanium alloys},
number = {February},
pages = {196--207},
title = {{The influence of shot peening on the fatigue response of Ti-6Al-4V surfaces subject to different machining processes}},
volume = {111},
year = {2018}
}
@article{Torres2002,
abstract = {Shot peening is a method widely used to improve the fatigue strength of materials, through the creation of a compressive residual stress field (CRSF) in their surface layers. In the present research the gain in fatigue life of AISI 4340 steel, used in landing gear, is evaluated under four shot peening conditions. Rotating bending fatigue tests were conducted and the CRSF was measured by an X-ray tensometry prior and during fatigue tests. It was observed that relaxation of the CRSF occurred due to the fatigue process. In addition, the fractured fatigue specimens were investigated using a scanning electron microscope in order to obtain information about the crack initiation points. The evaluation of fatigue life, relaxation of CRSF and crack sources are discussed. {\textcopyright} 2002 Elsevier Science Ltd. All rights reserved.},
